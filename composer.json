{
    "name": "kfoobar/laravel-restful",
    "description": "Boilerplate for Restful API in Laravel",
    "keywords": [
        "laravel",
        "restful",
        "api"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/config": "^8.0|^9.0|^10.0",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "stubs/app/",
            "KFoobar\\Restful\\": "src/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "authors": [
        {
            "name": "David Villa",
            "email": "david@kfoobar.se"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "KFoobar\\Restful\\ServiceProvider"
            ],
            "aliases": {
                "Restful": "KFoobar\\Restful\\Facades\\Restful"
            }
        }
    }
}
